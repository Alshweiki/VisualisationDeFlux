<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="hes_so.visualisationdeflux.Main0Activity">

    <TextView android:text="Réalisé Par" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text" />

    <TextView android:text="Mhd Ali Alshweiki" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/nom2"
        android:layout_gravity="center_vertical"
        android:layout_above="@+id/nom4"
        android:layout_alignRight="@+id/nom3"
        android:layout_alignEnd="@+id/nom3"
        android:layout_marginBottom="73dp" />
    <TextView android:text="Damien Goetschi" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/nom3"
        android:layout_below="@+id/nom2"
        android:layout_marginTop="42dp"
        android:layout_toRightOf="@+id/text"
        android:layout_toEndOf="@+id/text" />
    <TextView android:text="Quentin Matthewson" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/nom4"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/nom2"
        android:layout_alignStart="@+id/nom2" />

    <TextView
        android:text="Florent Aeby "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView"
        android:layout_gravity="center_vertical"
        android:layout_below="@+id/text"
        android:layout_alignLeft="@+id/nom2"
        android:layout_alignStart="@+id/nom2"
        android:layout_marginTop="42dp" />


</RelativeLayout>
